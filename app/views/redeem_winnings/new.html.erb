<%= form_for(@redeem_winning) do |f| %>
    <%= link_to 'cancel', redeem_winnings_path, id: 'cancel' %> Redeem<br/>
    You about to redeem
    <b><%= @redeem_winning.prize_amount %> prize points</b>
    for
    <i><%= @redeem_winning.prize_amount %>
      <%= @redeem_winning.prize_type == 'moola' ? "moola" : "clue points" %>
    </i>
    <br/>
    <%= f.hidden_field :prize_type %>
    <%= f.hidden_field :prize_amount %>
    <%= f.hidden_field :state %>
    <%= f.submit 'redeem', id: 'submit' %>
<% end %>
