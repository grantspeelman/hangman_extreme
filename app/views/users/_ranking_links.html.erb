<% ['daily','weekly','monthly'].each do |time_period| %>
    <% ['wins','rating','precision'].each do |rank| %>
        <%= link_to_unless params[:period] == time_period && params[:rank_by] == rank,
                           "#{time_period} #{rank}",
                           users_path(period: time_period, rank_by: rank),
                           id: "#{time_period}_#{rank}" %> |
    <% end %>
<% end %>